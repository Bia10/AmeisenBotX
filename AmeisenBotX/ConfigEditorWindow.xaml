                                <SolidColorBrush Color="{DynamicResource DarkForeground}" />
                            </Label.Foreground>
                        </Label>
                        <Label x:Name="labelCharacterSlot_Copy" Content="Port:" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Bahnschrift Light" Margin="244,217,0,0" Width="180" Padding="0,5,5,5">
                            <Label.Foreground>
                                <SolidColorBrush Color="{DynamicResource DarkForeground}" />
                            </Label.Foreground>
                        </Label>
                        <TextBox x:Name="textboxNavmeshServerPort" HorizontalAlignment="Left" Height="18" Margin="244,247,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="180" FontFamily="Bahnschrift Light" FontSize="14" BorderThickness="1,0,0,1" TabIndex="2" Style="{DynamicResource TextBoxStyleDark}">
                            <TextBox.BorderBrush>
                                <SolidColorBrush Color="{DynamicResource DarkBorder}" />
                            </TextBox.BorderBrush>
                            <TextBox.Foreground>
                                <SolidColorBrush Color="{DynamicResource DarkForeground}" />
                            </TextBox.Foreground>
                            <TextBox.Background>
                                <SolidColorBrush Color="{DynamicResource DarkBackground}" />
                            </TextBox.Background>
                        </TextBox>
                        <CheckBox x:Name="checkboxAutoPositionWow" Content="AutoPosition WoW (BETA)" HorizontalAlignment="Left" Margin="471,209,0,0" VerticalAlignment="Top" TabIndex="16" Style="{DynamicResource CheckBoxStyleDark}" FontFamily="Bahnschrift Light" VerticalContentAlignment="Bottom" Width="174">
                            <CheckBox.Foreground>
                                <SolidColorBrush Color="{DynamicResource DarkForeground}" />
                            </CheckBox.Foreground>
                            <CheckBox.BorderBrush>
                                <SolidColorBrush Color="{DynamicResource DarkBorder}" />
                            </CheckBox.BorderBrush>
                            <CheckBox.Background>
                                <SolidColorBrush Color="{DynamicResource DarkBackground}" />
                            </CheckBox.Background>
                        </CheckBox>
                        <Label x:Name="labelAutologin" Content="Auto Login" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Bahnschrift Light" Margin="4,80,0,0" BorderBrush="{DynamicResource Button.MouseOver.Border}" BorderThickness="0,0,0,1" Width="180" Padding="0,5,5,5">
                            <Label.Foreground>
                                <SolidColorBrush Color="{DynamicResource DarkForeground}" />
                            </Label.Foreground>
                        </Label>
                        <CheckBox x:Name="checkboxAutoLogin" Content="" HorizontalAlignment="Left" Margin="168,86,0,0" VerticalAlignment="Top" TabIndex="8" Style="{DynamicResource CheckBoxStyleDark}" FontFamily="Bahnschrift Light" Width="17" IsChecked="True" Background="{x:Null}" VerticalContentAlignment="Bottom" Padding="4,0,0,0">
                            <CheckBox.Foreground>
                                <SolidColorBrush Color="{DynamicResource DarkForeground}" />
                            </CheckBox.Foreground>
                            <CheckBox.BorderBrush>
                                <SolidColorBrush Color="{DynamicResource DarkBorder}" />
                            </CheckBox.BorderBrush>
                        </CheckBox>
                        <Label x:Name="labelAutostart" Content="Autostart" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Bahnschrift Light" Margin="244,4,0,0" BorderBrush="{DynamicResource Button.MouseOver.Border}" BorderThickness="0,0,0,1" Width="408" Padding="0,5,5,5">
                            <Label.Foreground>
                                <SolidColorBrush Color="{DynamicResource DarkForeground}" />
                            </Label.Foreground>
                        </Label>
                        <Label x:Name="labelNavmeshServer" Content="Navmesh Server" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Bahnschrift Light" Margin="244,138,0,0" BorderBrush="{DynamicResource Button.MouseOver.Border}" BorderThickness="0,0,0,1" Width="180" Padding="0,5,5,5">
                            <Label.Foreground>
                                <SolidColorBrush Color="{DynamicResource DarkForeground}" />
                            </Label.Foreground>
                        </Label>
                        <Label x:Name="labelWindowPositions" Content="Window Positions" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Bahnschrift Light" Margin="472,138,0,0" BorderBrush="{DynamicResource Button.MouseOver.Border}" BorderThickness="0,0,0,1" Width="180" Padding="0,5,5,5">
                            <Label.Foreground>
                                <SolidColorBrush Color="{DynamicResource DarkForeground}" />
                            </Label.Foreground>
                        </Label>
                    </Grid>
                </Border>
            </TabItem>
            <TabItem x:Name="tabitemGameplay" Header="Gameplay" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White">
                <Border BorderThickness="0,1,0,0" Margin="0,-3,0,0">
                    <Border.BorderBrush>
                        <SolidColorBrush Color="{DynamicResource DarkForeground}" />
                    </Border.BorderBrush>
                    <Grid>
                        <Label x:Name="labelLooting" Content="Looting" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Bahnschrift Light" Margin="244,4,0,0" BorderBrush="{DynamicResource Button.MouseOver.Border}" BorderThickness="0,0,0,1" Width="180" Padding="0,5,5,5">
                            <Label.Foreground>
                                <SolidColorBrush Color="{DynamicResource DarkForeground}" />
                            </Label.Foreground>
                        </Label>
                        <Slider x:Name="sliderMaxFollowDistance" HorizontalAlignment="Left" Margin="4,194,0,0" VerticalAlignment="Top" Width="179" Maximum="256" Minimum="4" SmallChange="1" Style="{DynamicResource SliderStyleDark}" ValueChanged="SliderMaxFollowDistance_ValueChanged" FontFamily="Bahnschrift Light" />
                        <Label x:Name="labelFollowing" Content="Following" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Bahnschrift Light" Margin="4,4,0,0" BorderThickness="0,0,0,1" Width="180" Padding="0,5,5,5">
