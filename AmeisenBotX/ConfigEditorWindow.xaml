                            <CheckBox.Foreground>
                                <SolidColorBrush Color="{DynamicResource DarkForeground}" />
                            </CheckBox.Foreground>
                            <CheckBox.BorderBrush>
                                <SolidColorBrush Color="{DynamicResource DarkBorder}" />
                            </CheckBox.BorderBrush>
                            <CheckBox.Background>
                                <SolidColorBrush Color="{DynamicResource DarkBackground}" />
                            </CheckBox.Background>
                        </CheckBox>
                        <PasswordBox x:Name="textboxPassword" HorizontalAlignment="Left" Margin="4,247,0,0" VerticalAlignment="Top" Width="180" Height="18" BorderThickness="1,0,0,1" TabIndex="3" Style="{DynamicResource PasswordBoxStyleDark}" FontFamily="Bahnschrift Light">
                            <PasswordBox.BorderBrush>
                                <SolidColorBrush Color="{DynamicResource DarkBorder}" />
                            </PasswordBox.BorderBrush>
                            <PasswordBox.Foreground>
                                <SolidColorBrush Color="{DynamicResource DarkForeground}" />
                            </PasswordBox.Foreground>
                            <PasswordBox.Background>
                                <SolidColorBrush Color="{DynamicResource DarkBackground}" />
                            </PasswordBox.Background>
                        </PasswordBox>
                        <Button x:Name="buttonOpenWowExe" Content="📁 Open WoW.exe" HorizontalAlignment="Left" Margin="244,100,0,0" VerticalAlignment="Top" Width="100" Height="18" Click="ButtonOpenWowExe_Click" TabIndex="7" FontSize="10" Style="{DynamicResource ButtonStyleDark}" FontFamily="Bahnschrift Light">
                            <Button.BorderBrush>
                                <SolidColorBrush Color="{DynamicResource DarkBorder}" />
                            </Button.BorderBrush>
                            <Button.Foreground>
                                <SolidColorBrush Color="{DynamicResource DarkForeground}" />
                            </Button.Foreground>
                            <Button.Background>
