                    <Border.BorderBrush>
                        <SolidColorBrush Color="{DynamicResource DarkForeground}" />
                    </Border.BorderBrush>
                    <Grid>
                        <TextBox x:Name="textboxConfigName" HorizontalAlignment="Left" Height="18" Margin="4,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="180" FontFamily="Bahnschrift Light" FontSize="14" BorderThickness="1,0,0,1" TabIndex="0" Style="{DynamicResource TextBoxStyleDark}">
                            <TextBox.BorderBrush>
                                <SolidColorBrush Color="{DynamicResource DarkBorder}" />
                            </TextBox.BorderBrush>
                            <TextBox.Foreground>
                                <SolidColorBrush Color="{DynamicResource DarkForeground}" />
                            </TextBox.Foreground>
                            <TextBox.Background>
                                <SolidColorBrush Color="{DynamicResource DarkBackground}" />
                            </TextBox.Background>
                        </TextBox>
                        <TextBox x:Name="textboxUsername" HorizontalAlignment="Left" Height="18" Margin="4,141,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="180" FontFamily="Bahnschrift Light" FontSize="14" BorderThickness="1,0,0,1" TabIndex="1" Style="{DynamicResource TextBoxStyleDark}">
                            <TextBox.BorderBrush>
                                <SolidColorBrush Color="{DynamicResource DarkBorder}" />
                            </TextBox.BorderBrush>
                            <TextBox.Foreground>
                                <SolidColorBrush Color="{DynamicResource DarkForeground}" />
                            </TextBox.Foreground>
                            <TextBox.Background>
                                <SolidColorBrush Color="{DynamicResource DarkBackground}" />
                            </TextBox.Background>
                        </TextBox>
                        <Label x:Name="labelConfigName" Content="Config Name" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Bahnschrift Light" Margin="4,4,0,0" BorderThickness="0,0,0,1" Width="180" Padding="0,5,5,5">
                            <Label.BorderBrush>
                                <SolidColorBrush Color="{DynamicResource DarkAccent1}" />
                            </Label.BorderBrush>
                            <Label.Foreground>
                                <SolidColorBrush Color="{DynamicResource DarkForeground}" />
                            </Label.Foreground>
                        </Label>
                        <Label x:Name="labelUsername" Content="Username:" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Bahnschrift Light" Margin="4,111,0,0" Width="180" Padding="0,5,5,5">
                            <Label.Foreground>
                                <SolidColorBrush Color="{DynamicResource DarkForeground}" />
                            </Label.Foreground>
                        </Label>
                        <Label x:Name="labelPassword" Content="Password:" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Bahnschrift Light" Margin="4,217,0,0" Width="186" Padding="0,5,5,5">
