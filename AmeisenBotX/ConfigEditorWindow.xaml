                        <TextBox x:Name="textboxWowPath" HorizontalAlignment="Left" Height="18" Margin="244,75,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="408" FontFamily="Bahnschrift Light" FontSize="14" BorderThickness="1,0,0,1" TabIndex="6" Style="{DynamicResource TextBoxStyleDark}">
                            <TextBox.BorderBrush>
                                <SolidColorBrush Color="{DynamicResource DarkBorder}" />
                            </TextBox.BorderBrush>
                            <TextBox.Foreground>
                                <SolidColorBrush Color="{DynamicResource DarkForeground}" />
                            </TextBox.Foreground>
                            <TextBox.Background>
                                <SolidColorBrush Color="{DynamicResource DarkBackground}" />
                            </TextBox.Background>
